{
  "name": "vite-react-typescript-tailwind-starter",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm run build-functions && DEBUG=app* netlify dev",
    "devrun": "vite",
    "build": "npm run build-functions && tsc && vite build",
    "build-functions": "tsc -p ./functions/tsconfig.json",
    "serve": "vite preview --port 3000",
    "local": "DEBUG=app* npm run build-functions && npm run local-start",
    "local-start": "node functions/dist/functions/studio-api/start-local.js",
    "invoke-local": "ntl functions:invoke --port 8888 studio-api",
    "clean": "rm -rf ./functions/dist && rm -rf ./dist",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint-functions": "eslint ./functions --ext .ts",
    "lint-src": "eslint ./src --ext .ts,.tsx,.js",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "route-harness": "^2.1.5",
    "serverless-http": "^3.0.1",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@heroicons/react": "^1.0.5",
    "@swc/core": "^1.2.204",
    "@tailwindcss/forms": "^0.4.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.18.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^8.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "tailwindcss": "^3.0.7",
    "ts-jest": "^27.1.2",
    "typescript": "^4.7.4",
    "vite": "^2.7.6",
    "vite-plugin-node": "^1.0.0"
  }
}
